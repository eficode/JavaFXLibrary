<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<fx:root type="javafx.scene.layout.VBox"
         xmlns:fx="http://javafx.com/fxml"
         fx:controller="javafxlibrary.testapps.controllers.ImageDemoController">
    <HBox fx:id="searchContainer">
        <TextField fx:id="search" promptText="Search" onKeyReleased="#searchListener"/>
    </HBox>
    <ScrollPane fx:id="searchResults">
        <VBox fx:id="rowWrapper"/>
    </ScrollPane>
</fx:root>
