<fx:root type="javafx.scene.layout.VBox" xmlns:fx="http://javafx.com/fxml">
    <HBox spacing="50">
        <StackPane>
            <Rectangle fx:id="rectangle" width="215" height="215"/>
            <Pane>
                <Line startX="1.0" startY="1.0" endX="95.0" endY="95.0" />
                <Line startX="120.0" startY="120.0" endX="215.0" endY="215.0" />
                <Line startX="215.0" startY="1.0" endX="120.0" endY="95.0" />
                <Line startX="95.0" startY="120.0" endX="1.0" endY="215.0" />
            </Pane>
            <Text text="NO IMAGE AVAILABLE" />
        </StackPane>
        <VBox>
            <Label fx:id="headingLabel" text="Heading"/>
            <Label fx:id="contentLabel" styleClass="textRow" text="Text content" />
        </VBox>
    </HBox>
</fx:root>
