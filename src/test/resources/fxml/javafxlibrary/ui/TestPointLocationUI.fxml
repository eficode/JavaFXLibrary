<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="javafxlibrary.testapps.controllers.TestPointLocationController"
            prefHeight="500.0" prefWidth="500.0"
            onMouseMoved="#mouseListener"
            onMouseExited="#mouseExitedListener">
    <VBox AnchorPane.bottomAnchor="0" AnchorPane.leftAnchor="0" AnchorPane.rightAnchor="0" AnchorPane.topAnchor="0">
        <HBox>
            <Label fx:id="locationLabel" />
        </HBox>
    </VBox>
    <Rectangle fx:id="rectangle" height="50" width="50" fill="lawngreen"
               AnchorPane.bottomAnchor="0" AnchorPane.leftAnchor="0"/>

    <stylesheets>
        <URL value="@css/TestPointLocation.css" />
    </stylesheets>

</AnchorPane>
